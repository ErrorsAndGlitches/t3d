% simple figure, half page
\newcommand{\fig}[3]{\begin{figure}[!h]\centering\includegraphics[width=0.5\textwidth]{#1/#2.png}\caption{#3}
	\label{fig:#2}\end{figure}}
\newcommand{\figw}[4]{\begin{figure}[!h]\centering\includegraphics[width=#4\textwidth]{#1/#2.png}\caption{#3}
	\label{fig:#2}\end{figure}}
\newcommand{\figwpdf}[4]{\begin{figure}[!h]\centering\includegraphics[width=#4\textwidth]{#1/#2.pdf}\caption{#3}
	\label{fig:#2}\end{figure}}

% wrap figure: 
%    1) folder     
%    2) image name (w/o png)    
%    3) caption     
%    4) alignment     
%    5) width of figure (0.5)
%    6) width of image in figure (.45)
\newcommand{\wrapfig}[6]{\begin{wrapfigure}{#4}{#5\textwidth}\begin{center}\includegraphics[width=#6\textwidth]{#1/#2.png}
	\end{center}\caption{#3}\label{fig:#2}\end{wrapfigure}}
\newcommand{\wrapfigpdf}[6]{\begin{wrapfigure}{#4}{#5\textwidth}\begin{center}\includegraphics[width=#6\textwidth]{#1/#2.pdf}
	\end{center}\caption{#3}\label{fig:#2}\end{wrapfigure}}

